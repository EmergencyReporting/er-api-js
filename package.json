{
  "name": "@ercorp/er-api-js",
  "version": "1.0.0",
  "description": "Emergency Reporting Auth and API client library.",
  "repository": {
    "type": "git",
    "url": "https://github.com/EmergencyReporting/er-api-js.git"
  },
  "scripts": {
    "docs": "npm run docs:clean && ./node_modules/.bin/jsdoc -c ./.jsdoc.json --readme ./README.md --package ./package.json",
    "docs:clean": "rimraf ./docs",
    "docs:publish": "mkdir .\\docs\\assets && copy .\\assets .\\docs\\assets && gh-pages -d ./docs",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack",
    "build:clean": "rimraf ./dist",
    "build:publish": "mkdir .\\docs\\",
    "clean": "npm run build:clean && npm run docs:clean"
  },
  "directories": {
    "lib": "./dist"
  },
  "files": [
    "dist/*"
  ],
  "main": "dist/erapijs.node.commonjs2.js",
  "module": "src/index.js",
  "author": "",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "axios": "^0.18.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "bluebird": "^3.5.1",
    "gh-pages": "^1.1.0",
    "jsdoc": "^3.5.5",
    "tui-jsdoc-template": "^1.2.2",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15"
  }
}
