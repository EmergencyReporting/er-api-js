{
  "name": "@ercorp/er-api-js",
  "version": "0.8.0",
  "description": "Emergency Reporting Auth and API client library.",
  "repository": {
    "type": "git",
    "url": "https://github.com/EmergencyReporting/er-api-js.git"
  },
  "scripts": {
    "docs":
      "npm run docs:clean && ./node_modules/.bin/jsdoc -c ./.jsdoc.json --readme ./README.md --package ./package.json",
    "docs:clean":
      "rm -rf ./docs && mkdir ./docs && cp -R ./assets ./docs/assets",
    "docs:publish": "gh-pages -d ./docs",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "directories": {
    "lib": "./lib"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "bluebird": "^3.5.1"
  },
  "devDependencies": {
    "gh-pages": "^1.1.0",
    "jsdoc": "^3.5.5",
    "tui-jsdoc-template": "^1.2.2"
  }
}
